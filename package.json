{
  "name": "graphql-server",
  "licence": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/platyplus/graphql-server"
  },
  "scripts": {
    "start": "node ./dist/index.js",
    "start-dev": "dotenv -- nodemon -e ts,graphql -x ts-node src/index.ts",
    "dev": "npm-run-all --parallel start-dev playground",
    "test": "echo 'tests to be defined'; exit 0",
    "debug": "dotenv -- nodemon -e ts,graphql -x ts-node --inspect src/index.ts",
    "playground": "graphql playground",
    "build": "rimraf dist && tsc"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "base64-js": "^1.3.0",
    "bcryptjs": "2.4.3",
    "graphql-shield": "^3.2.5",
    "graphql-yoga": "1.16.2",
    "js-sha1": "^0.6.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.11",
    "prisma-binding": "2.1.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.1",
    "@types/lodash": "^4.14.116",
    "dotenv-cli": "1.4.0",
    "graphql-cli": "2.16.7",
    "nodemon": "1.18.4",
    "npm-run-all": "4.1.3",
    "prisma": "1.15.3",
    "rimraf": "2.6.2",
    "ts-node": "6.2.0",
    "typescript": "2.9.2"
  }
}
